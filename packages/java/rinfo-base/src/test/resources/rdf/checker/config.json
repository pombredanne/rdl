{
    "http://purl.org/dc/terms/": {
        "properties": {
            "title": {
                "requireLang": "en",
                "strictWhitespace": true
            },
            "created": {
                "datatype": "date",
                "dateConstraint": {
                    "maxDaysFromNow": 0
                }
            },
            "issued": {
                "datatype": "date",
                "dateConstraint": {
                    "minYear": 1600,
                    "maxYearsFromNow": 20
                }
            },
            "references": {
                "reference": true
            }
        }
    },
    "http://example.org/def/vocab#": {
        "classes": {
            "Document": {}
        },
        "properties": {
            "token": {},
            "changes": {
                "reference": true,
                "patternRef": "publUrl"
            }
        }
    },
    "patterns": {
        "publUrl": "http://example.org/publ/[0-9a-z]+"
    }
}
