{
    "prefixes": {
        "dct": "http://purl.org/dc/terms/",
        "": "http://example.org/def/vocab#"
    },
    "classes": {
        "Document": {}
    },
    "properties": {
        "token": {},
        "dct:title": {
            "requireLang": "en",
            "strictWhitespace": true
        },
        "dct:created": {
            "datatype": "xsd:date",
            "dateConstraint": {
                "maxDaysFromNow": 0
            }
        },
        "dct:issued": {
            "datatype": "xsd:date",
            "dateConstraint": {
                "minYear": 1600,
                "maxYearsFromNow": 20
            }
        },
        "dct:references": {
            "reference": true
        },
        "changes": {
            "reference": true,
            "patternRef": "fsPubl"
        }
    },
    "patterns": {
        "fsPubl": "http://example.org/publ/[0-9a-z]+"
    }
}
