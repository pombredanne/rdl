{
    "prefixes": {
        "dct": "http://purl.org/dc/terms/",
        "": "http://example.org/def/vocab#"
    },
    "classes": {
        "Document": {}
    },
    "properties": {
        "token": {},
        "dct:title": {
            "requireLang": "en",
            "strictWhitespace": true
        },
        "dct:created": {
            "datatype": "xsd:date"
        },
        "dct:issued": {
            "datatype": "xsd:date",
            "pattern": "(^1[6-9]|^2[0-5])\\d{2}.*"
        },
        "dct:references": {
            "reference": true
        },
        "changes": {
            "reference": true,
            "patternRef": "fsPubl"
        }
    },
    "patterns": {
        "fsPubl": "http://example.org/publ/[0-9a-z]+"
    }
}
