{
    "http://purl.org/dc/terms/": {
        "properties": {
            "title": {
                "requireLang": "sv",
                "strictWhitespace": true
            },
            "created": {
                "datatype": "date",
                "dateConstraint": {
                    "maxDaysFromNow": 0
                }
            },
            "issued": {
                "datatype": "date",
                "dateConstraint": {
                    "minYear": 1600,
                    "maxYearsFromNow": 20
                }
            }
        }
    },
    "http://rinfo.lagrummet.se/ns/2008/11/rinfo/publ#": {
        "properties": {
            "andrar": {
                "reference": true,
                "pattern": "http://rinfo.lagrummet.se/publ/[0-9a-z]+fs/[1-9][0-9/]+:\\w+"
            }
        }
    }
}
