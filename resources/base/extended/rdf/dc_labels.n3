@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .

dct:title a rdf:Property;
    rdfs:label "Titel"@sv;
    rdfs:comment "Dokumentets officiella titel"@sv .

dct:description a rdf:Property;
    rdfs:label "Beskrivning"@sv;
    rdfs:comment "Text som beskriver dokumentet."@sv .

dct:publisher a rdf:Property;
    rdfs:label "Utgivare"@sv;
    rdfs:comment "Ansvarig utgivare av detta dokument."@sv .

dct:creator a rdf:Property;
    rdfs:label "Skapare av dokumentet"@sv;
    rdfs:comment ""@sv .

dct:subject a rdf:Property;
    rdfs:label "Ämnesord"@sv;
    rdfs:comment "Resursens ämne/tema"@sv .

dct:references a rdf:Property;
    rdfs:label "Hänvisar"@sv;
    rdfs:comment "Någon form av hänvisning (t.ex. ett annat dokument som omnämns i texten)."@sv;
    owl:inverseOf [ rdfs:label "Hänvisad av"@sv ]; .

dct:created a rdf:Property;
    rdfs:label "Skapad"@sv;
    rdfs:comment ""@sv .

dct:modified a rdf:Property;
    rdfs:label "Senast ändrad"@sv;
    rdfs:comment ""@sv .

dct:issued a rdf:Property;
    rdfs:label "Utgiven"@sv;
    rdfs:comment "Datum då resursen formellt utgavs (t.ex. publicering)."@sv .

dct:identifier a rdf:Property;
    rdfs:label "Identifierare"@sv;
    rdfs:comment "En otvetydig hänvisning till resursen inom en givet kontext."@sv .

dct:alternative a rdf:Property;
    rdfs:label "Alternativ titel"@sv;
    rdfs:comment "Ett alternativt namn för resursen."@sv .

