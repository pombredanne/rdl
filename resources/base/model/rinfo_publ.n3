# Ontology
#-----------------------------------------------------------------------

: a owl:Ontology;
    dct:title "Svenska rättsinformationssystemets dokumentmodell."@sv;
    dct:description """En begreppsmodell som beskriver de dokumenttyper och
                    andra relaterade ting, inklusive egenskaper, som ingår i det svenska
                    rättsinformationssystemet."""@sv;
    dct:created "2006-12-05"^^xsd:date;
    owl:versionInfo "0.4 rev 1";
    owl:imports dct:, skos:, foaf:, protege: .


##
# Grund
#-----------------------------------------------------------------------

:Rattsinformationsdokument a owl:Class;
    rdfs:subClassOf foaf:Document;
    rdfs:label "Rättsinformationsdokument"@sv;
    rdfs:label "Swedish Legal Document"@en;
    rdfs:comment "Post eller dokument i det svenska offentliga rättsinformationssystemet."@sv;
    protege:abstract "true";
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty dct:title;
            owl:allValuesFrom rdfs:Literal;
            owl:minCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty dct:description;
            owl:allValuesFrom rdfs:Literal;
            owl:minCardinality 0
        ],
        [ a owl:Restriction;
            owl:onProperty dct:issued;
            owl:cardinality 1;
            # TODO: strukturerad kommentar?
            #rdfs:comment "Är i regel alltid publicerad, men datumet kan vara okänt."@sv
        ],
        [ a owl:Restriction;
            owl:onProperty dct:references;
            owl:allValuesFrom rdfs:Resource;
            owl:minCardinality 0
        ],
        [ a owl:Restriction;
            owl:onProperty dct:publisher;
            owl:allValuesFrom foaf:Agent;
            owl:maxCardinality 1;
        ],
        [ a owl:Restriction;
            owl:onProperty :bilaga;
            owl:minCardinality 0
        ],
        # TODO: okej? för att för att kunna ange info om var man kan läsa mer
        # om dokumentet *eller hur det/dess innnehåll kan hittas*. Pekar på
        # lite vad som, t.ex. en tipssida el.dyl.
        [ a owl:Restriction;
            owl:onProperty rdfs:seeAlso;
            owl:allValuesFrom rdfs:Resource;
            owl:minCardinality 0
        ] ;
    rdfs:isDefinedBy : .


:bilaga a owl:ObjectProperty;
    rdfs:subPropertyOf dct:hasPart;
    rdfs:label "Bilaga"@sv;
    rdfs:comment "En generell bilaga till något rättsinformationsdokument."@sv;
    owl:inverseOf [ rdfs:label "Bilaga till"@sv ];
    rdfs:isDefinedBy : .

:rattsdokumentnummer a owl:DatatypeProperty;
    rdfs:label "Numrering av svenskt rättsinformationsdokument"@sv;
    rdfs:label "Swedish Legal Document Number"@en;
    rdfs:comment "Abstrakt numrering av ett dokument i det svenska offentliga rättsinformationssystemet."@sv;
    protege:abstract "true";
    # TODO: är det värdefullt att ange ett strikt format här?
    rdfs:isDefinedBy : .


:publikationssamling a owl:ObjectProperty;
    rdfs:subPropertyOf dct:isPartOf;
    rdfs:label "Publikationssamling"@sv;
    rdfs:comment """Anger medlemsskap i någon form av (rättsinformationsspecifik) publikationssamling, såsom en författningssamling eller utredningsserie."""@sv;
    protege:abstract "true";
    rdfs:isDefinedBy : .


# TODO: använd denna eller bara någon DC-term?
