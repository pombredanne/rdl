##
# Ontology
#-----------------------------------------------------------------------

: a owl:Ontology;
    dct:title "Svenska rättsinformationssystemets dokumentmodell"@sv;
    dct:description """En begreppsmodell som beskriver de dokumenttyper,
                    egenskaper och andra relaterade ting som ingår i det
                    svenska rättsinformationssystemet."""@sv;
    dct:created "2006-12-05"^^xsd:date;
    dct:updated "2010-05-19"^^xsd:date;
    owl:versionInfo "0.6";
    owl:imports dct:, skos:, foaf: . # TODO:? does "imports" imply too much?


##
# Bas
#-----------------------------------------------------------------------

:Rattsinformationsdokument a owl:Class;
    rdfs:subClassOf foaf:Document;
    rdfs:label "Rättsinformationsdokument"@sv;
    rdfs:label "Swedish Legal Document"@en;
    rdfs:comment "Post eller dokument i det svenska offentliga rättsinformationssystemet."@sv;
    protege:abstract "true";
    rdfs:subClassOf
        # Alla dokument har inte dct:title (exv :Rattsfallsreferat,
        # som däremot har subegenskapen :referatrubrik)
        [ a owl:Restriction;
            owl:onProperty dct:title;
            owl:allValuesFrom rdfs:Literal;
            owl:minCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty dct:issued;
            owl:allValuesFrom xsd:date;
            owl:cardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty dct:publisher;
            owl:allValuesFrom foaf:Organization;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty dct:identifier;
            owl:someValuesFrom rdfs:Literal;
            #owl:minCardinality 0;  # TODO: Alla resurser kommer inte ha en
                                    # etablerad identifierare (exv vägledande
                                    # avgöranden)
        ];
    rdfs:isDefinedBy : .


:bilaga a owl:ObjectProperty;
    rdfs:subPropertyOf dct:hasPart;
    rdfs:label "Bilaga"@sv;
    rdfs:comment "En generell bilaga till ett rättsinformationsdokument."@sv;
    rdfs:domain :Rattsinformationsdokument;
    owl:inverseOf :bilagaTill;
    rdfs:isDefinedBy : .

:bilagaTill a owl:ObjectProperty;
    rdfs:subPropertyOf dct:isPartOf;
    rdfs:label "Bilaga till"@sv;
    owl:inverseOf :bilaga;
    rdfs:isDefinedBy : .


:publikationssamling a owl:ObjectProperty;
    rdfs:subPropertyOf dct:isPartOf;
    rdfs:label "Publikationssamling"@sv;
    rdfs:comment """Anger medlemsskap i någon form av (rättsinformationsspecifik) publikationssamling, såsom en författningssamling eller utredningsserie."""@sv;
    protege:abstract "true";
    rdfs:isDefinedBy : .

:arsutgava a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Årsutgåva"@sv;
    rdfs:comment """En viss årsutgåva av någon publikation. Exempelvis "2005"."""@sv;
    # rdfs:range xsd:gYear; . # kan vara även s.k. brutet år, exv "1975/76"
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy : .

:lopnummer a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Löpnummer"@sv;
    rdfs:comment """Löpnummer i någon publikation (exempelvis "57")."""@sv;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy : .

:sidnummer a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Sidnummer"@sv;
    rdfs:comment """Sidnummer i någon publikation. Exempelvis "523"."""@sv;
    # rdfs:range xsd:nonNegativeInteger; . # Alla sidor numreras inte med positiva heltal
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy : .


# En fallback-egenskap om vi inte kan ange årsutgåva,
# löpnummer/sidnummer etc.
# Vi avvvaktar med att introducera :platsangivelse tills reellt behov uppstår.
#:platsangivelse a owl:DatatypeProperty;
#    rdfs:isDefinedBy : ;
#    rdfs:label "Platsangivelse"@sv;
#    rdfs:comment """En generell hänvisning till en plats i någon publikation (exempelvis "1899:bih. 40 s. 3", "1913 B 418.", "1978 2:37"). Ska endast användas om de mer specifika hänvisningsegenskaperna :arsutgava, :sidnummer, :lopnummer etc inte är tillgängliga. Ska inkludera årtal om sådant finns, men inte publikationsidentifierare (såsom "SFS", "NJA" eller "RÅ")"""@sv;
#    rdfs:isDefinedBy : .


##
# Författningar
#-----------------------------------------------------------------------

:FSDokument a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Författningssamlingsdokument"@sv;
    rdfs:comment "Ett dokument som ingår i någon författningssamling."@sv;
    protege:abstract "true";
    rdfs:subClassOf :Rattsinformationsdokument;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :arsutgava;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :lopnummer;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :sidnummer;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :bihangsnummer;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :forfattningssamling;
            owl:cardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :utkomFranTryck;
            owl:maxCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :overgangsbestammelse;
            owl:maxCardinality 1
        ] .


:forfattningssamling a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :publikationssamling;
    rdfs:label "Författningssamling"@sv;
    rdfs:comment """Författningssamlingen som dokumentet publiceras i."""@sv;
    rdfs:range :Forfattningssamling; .

:utkomFranTryck a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf dct:issued;
    rdfs:label "Utkom från trycket"@sv;
    rdfs:comment "Datum då dokumentet utkom från trycket."@sv;
    rdfs:range xsd:date; .

# Förekommer endast i SFS (såvitt har observerats)
:bihangsnummer a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Bihangsnummer"@sv;
    rdfs:comment """Nummer på det bihang som dokumentet återfinns i. Exempelvis "40"."""@sv; .


:beslutande a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Beslutande"@sv;
    rdfs:comment "Referens till beslutande (signent/undertecknare) av dokumentet."@sv;
    rdfs:domain :FSDokument;
    rdfs:range foaf:Person; .

:foredragande a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Föredragande"@sv;
    rdfs:comment "Referens till dokumentets föredragande (kontrasignent)."@sv;
    rdfs:domain :FSDokument;
    rdfs:range foaf:Person; .

:bemyndigande a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Bemyndigande"@sv;
    rdfs:comment "Referens till en bemyndigande rättsgrund som rättsregeldokumentet grundar sig på."@sv;
    rdfs:domain :FSDokument;
    owl:inverseOf [ rdfs:label "Bemyndigar"@sv ]; .

:genomforDirektiv a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Genomför direktiv"@sv;
    rdfs:comment "Referens till EU-direktiv som helt eller delvis genomförs med denna författning."@sv;
    rdfs:domain :FSDokument;
    owl:inverseOf [ rdfs:label "Genomförs av"@sv ]; .

:andrar a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Ändrar"@sv;
    rdfs:comment "Referens till något som ändras, t.ex. en förordning eller en del därav (såsom rubrik, kapitel, lagparagraf)."@sv;
    rdfs:domain :FSDokument;
    owl:inverseOf [ rdfs:label "Ändras av"@sv ]; .

:ersatter a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :andrar;
    #owl:equivalentProperty dct:replaces;
    rdfs:label "Ersätter"@sv;
    rdfs:comment "Anger något som ersätts av denna resurs, t.ex. en förordning eller en del därav (såsom rubrik, kapitel, lagparagraf)."@sv;
    owl:inverseOf [ rdfs:label "Ersätts av"@sv ]; .

:inforsI a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :andrar;
    rdfs:label "Införs i"@sv;
    rdfs:comment "Anger att innehållet som denna resurs representerar införs i t.ex. en förordning."@sv; .

:upphaver a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :andrar;
    rdfs:label "Upphäver"@sv;
    rdfs:comment "Referens till något som upphävs, t.ex. en förordning eller en del därav (såsom rubrik, kapitel, lagparagraf)."@sv;
    owl:inverseOf [ rdfs:label "Upphävd av"@sv ]; .

:forarbete a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf dct:references;
    rdfs:label "Förarbete"@sv;
    rdfs:comment "Relaterat förarbete till en författning."@sv;
    rdfs:domain :FSDokument;
    owl:inverseOf [ rdfs:label "Förarbete till"@sv ]; .

:ikrafttradanderegel a rdf:Property;
    rdfs:isDefinedBy : ;
    rdfs:label "Ikraftträdanderegel"@sv;
    rdfs:comment "En bestämmelse som anger om, när och hur författningen träder ikraft."@sv;
    rdfs:domain :FSDokument;
    protege:abstract "true" .

:ikrafttradandedatum a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :ikrafttradanderegel;
    rdfs:label "Ikraftträdandedatum"@sv;
    rdfs:comment "Datum då författningen träder ikraft."@sv;
    rdfs:range xsd:date; .

:ikrafttradandeanvisning a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :ikrafttradanderegel;
    rdfs:label "Ikraftträdandeanvisning"@sv;
    rdfs:comment """Referens till anvisning som anger hur författningen träder ikraft."""@sv .

:ikrafttradandetext a rdf:Property;
    rdfs:isDefinedBy : ;
    rdfs:subPropertyOf :ikrafttradanderegel;
    rdfs:label "Ikraftträdandetext"@sv;
    rdfs:comment """Anvisning som anger hur författningen träder ikraft - exempelvis "Träder ikraft den dagen regeringen bestämmer."."""@sv;
    rdfs:range rdfs:Literal; .

:ikrafttradandelagrum a owl:ObjectProperty;
    rdfs:subPropertyOf :ikrafttradanderegel;
    rdfs:label "Ikraftträdandelagrum"@sv;
    rdfs:comment "Hänvisning till lagrum (promulgationslag) som anger när och hur författningen träder ikraft."@sv;
    owl:inverseOf [ rdfs:label "Bestämmer ikraftträdande för"@sv ];
    rdfs:isDefinedBy : .

:overgangsbestammelse a rdf:Property;
    rdfs:label "Övergångsbestämmelse"@sv;
    rdfs:comment "Information om övergångsbestämmelser."@sv;
    protege:abstract "true";
    rdfs:domain :FSDokument;
    rdfs:isDefinedBy : .

:overgangsbestammelsehanvisning a owl:ObjectProperty;
    rdfs:subPropertyOf :overgangsbestammelse;
    rdfs:label "Övergångsbestämmelsehänvisning"@sv;
    rdfs:comment "Referens till något som beskriver övergångsbestämmelser."@sv;
    rdfs:isDefinedBy : .

:overgangsbestammelsetext a rdf:Property;
    rdfs:subPropertyOf :overgangsbestammelse;
    rdfs:label "Övergångsbestämmelsetext"@sv;
    rdfs:comment "Text som beskriver övergångsbestämmelser."@sv;
    rdfs:isDefinedBy : .

:beslutsdatum a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Beslutsdatum"@sv;
    rdfs:comment "Datum då innehållet i dokumentet beslutades."@sv;
    # TODO: se anv. i Restriction:s
    rdfs:domain [ owl:unionOf (:Tillkannagivande :Cirkular :Myndighetsforeskrift :Forarbete) ];
    rdfs:range xsd:date; .

# Enligt JuDep 070906 är dessa ekvivalenta.
:beslutsdatum owl:equivalentProperty :utfardandedatum .

:utfardandedatum a owl:DatatypeProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Utfärdandedatum"@sv;
    rdfs:comment "Datum då författningen utfärdades. Motsvarar beslutsdatum."@sv;
    # TODO: se anv. i Restriction:s
    rdfs:domain [ owl:unionOf (:Lag :Forordning :Brev) ];
    rdfs:range xsd:date; .

:omtryckAv a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:domain :FSDokument;
    rdfs:range :FSDokument;
    rdfs:label "Omtryck av"@sv;
    rdfs:comment "Referens till vad omtrycket avser, t.ex. en förordning."@sv;
    owl:inverseOf [ rdfs:label "Har omtryck"@sv ]; .


#---------------------------------------

:Forfattning a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Författning"@sv;
    rdfs:comment "En text som innehåller generella, bindande rättsregler (såsom lag, förordning eller föreskrift)  utgiven i någon författningssamling."@sv;
    protege:abstract "true";
    rdfs:subClassOf :FSDokument;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :beslutadAv;
            owl:minCardinality 1
        ] .


:beslutadAv a owl:ObjectProperty;
    rdfs:isDefinedBy : ;
    rdfs:label "Beslutad av"@sv;
    rdfs:comment "Referens till officiell beslutstagare."@sv;
    rdfs:domain :Forfattning;
    rdfs:range foaf:Agent .


#---------------------------------------

:Lag a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Lag"@sv;
    rdfs:comment "En författning av typen lag (d.v.s. antagen av riksdagen)."@sv;
    rdfs:subClassOf :Forfattning;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :utfardandedatum;
            owl:cardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :forarbete;
            owl:minCardinality 1;
        ] .


#---------------------------------------

:Grundlag a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Grundlag"@sv;
    rdfs:comment ""@sv;
    rdfs:comment "En författning av typen grundlag (d.v.s. antagen av två på varandra följande riksdagar)."@sv;
    rdfs:subClassOf :Lag ;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :utfardandedatum;
            owl:cardinality 2
        ] .


#---------------------------------------

:Forordning a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Förordning"@sv;
    rdfs:comment "En författning av typen förordning (d.v.s. antagen av regeringen)."@sv;
    rdfs:subClassOf :Forfattning;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :utfardandedatum;
            owl:cardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :bemyndigande;
            owl:minCardinality 1
        ] .


#---------------------------------------

:Myndighetsforeskrift a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Myndighetsföreskrift"@sv;
    rdfs:comment "En föreskrift utgiven i någon författningssamling."@sv;
    rdfs:subClassOf :Forfattning;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :bemyndigande;
            owl:minCardinality 1
        ],
        [ a owl:Restriction;
            owl:onProperty :beslutsdatum;
            owl:cardinality 1
        ] .


#---------------------------------------

:Tillkannagivande a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Tillkännagivande"@sv;
    rdfs:comment """Tillkännagivanden används när det finns behov av att upplysa
                 en större krets om faktiska förhållanden."""@sv;
    rdfs:subClassOf :FSDokument;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :beslutsdatum;
            owl:cardinality 1
        ] .


#---------------------------------------

:Cirkular a owl:Class;
    rdfs:isDefinedBy : ;
    rdfs:label "Cirkulär"@sv;
    rdfs:comment """Dokument för att upplysa en bestämd krets (företrädesvis
                    myndigheter) om innehållet i vissa normer eller att
                    informera addressaterna om förhållanden av intresse."""@sv;
    rdfs:subClassOf :FSDokument;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :beslutsdatum;
            owl:cardinality 1
        ] .


#---------------------------------------

:Brev a owl:DeprecatedClass;
    rdfs:isDefinedBy : ;
    rdfs:label "Brev"@sv;
    # rdfs:comment ""@sv; # TODO
    rdfs:subClassOf :FSDokument;
    rdfs:subClassOf
        [ a owl:Restriction;
            owl:onProperty :utfardandedatum;
            owl:cardinality 1
        ] .


