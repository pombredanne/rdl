@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix module: <http://joseki.org/2003/06/module#> .
@prefix joseki: <http://joseki.org/2005/06/configuration#> .
@prefix ja:     <http://jena.hpl.hp.com/2005/11/Assembler#> .

<> rdfs:label "Joseki Configuration File"@en .

<#server> a joseki:Server;
    joseki:initialization
        [ module:implementation
            [ module:className <java:org.joseki.util.ServiceInitSimple>;
              rdfs:label "Example initializer"@en ]
        ] .

<#fixed_service> a joseki:Service;
    rdfs:label          "Service point"@en;
    joseki:serviceRef   "sparql";
    joseki:processor    <#ProcessorSPARQL_FixedDS>;
    joseki:dataset      <#models> .

<#ProcessorSPARQL_FixedDS> a joseki:Processor;
    rdfs:label "SPARQL processor for fixed datasets"@en;
    module:implementation <#ImplSPARQL>;
    joseki:allowExplicitDataset       "false"^^xsd:boolean;
    joseki:allowWebLoading            "false"^^xsd:boolean;
    joseki:lockingPolicy              joseki:lockingPolicyMRSW .

<#ImplSPARQL> a joseki:ServiceImpl;
    module:className <java:org.joseki.processors.SPARQL> .

<#models> a ja:RDFDataset;
    rdfs:label "Model data"@en;
    ja:defaultGraph
      [ a ja:MemoryModel;

        # RInfo model
        ja:content [ja:externalContent <file:../../../resources/base/model/rinfo_kommitte.n3> ];
        ja:content [ja:externalContent <file:../../../resources/base/model/rinfo_publ.n3> ];

        # Extended Public Data
        # - swedish labels:
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/dc_labels.n3> ];
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/foaf_labels.n3> ];
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/misc_labels.n3> ];
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/skos_labels.n3> ];
        # - "missing" xsd data:
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/xsd.rdfs> ];
        # - missing Agent isDefinedBy foaf:
        ja:content [ja:externalContent <file:../../../resources/base/extended/rdf/foaf_added_info.n3> ];

        # NOTE: local copies of publiclay available ontologies
        #ja:content [ja:externalContent <file:../../../resources/external/rdf/atomowl.owl> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/bibo.owl> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/dc_elements.rdfs> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/dcterms.rdfs> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/foaf.owl> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/owl.owl> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/rdf-schema.rdfs> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/rdf.rdfs> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/sioc.owl> ];
        ja:content [ja:externalContent <file:../../../resources/external/rdf/skos_core.rdfs> ];

      ] .

