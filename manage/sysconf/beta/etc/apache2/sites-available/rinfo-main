<VirtualHost *:80>
    ServerName rinfo.beta.lagrummet.se
    ServerSignature On

    DocumentRoot /var/www/
    <Directory /var/www/>
        Options Indexes FollowSymLinks MultiViews
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

    ProxyPassMatch ^/robots\.txt$ !

    ProxyPass / ajp://localhost:8009/rinfo-main/
</VirtualHost>
