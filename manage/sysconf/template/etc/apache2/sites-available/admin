<VirtualHost *:80>

    ServerName www.admin.dnsplaceholderforsed
    ServerAlias admin.dnsplaceholderforsed
    DocumentRoot "/var/www/admin"
    <Directory "/var/www/admin">
        Options Indexes FollowSymLinks MultiViews
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

    ServerAdmin rinfo@lagrummet.se

    ErrorLog /var/log/httpd/admin.dnsplaceholderforsed.error.log
    LogLevel warn
    CustomLog /var/log/httpd/admin.dnsplaceholderforsed.requests.log combined
</VirtualHost>
